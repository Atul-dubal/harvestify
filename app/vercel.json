{
    "version": 2,
    "env": {
        "PYTHON_VERSION": "3.10"
    },
    "builds": [
        {
            "src": "./app.py",
            "use": "@vercel/python",
             "config": {
        "maxLambdaSize": "450mb"
      }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}
